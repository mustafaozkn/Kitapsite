@using Book.Mvc.Models;
@model IEnumerable<BookListModel>
@{
    ViewBag.Title = "Search";
}

<div class="container pb-5">

    <div class="row">
        <div class="col-md-9">
            <div class="title-bg">
                <div class="title">@ViewData["kelime"] sözcüğü için sonuçlar</div>
            </div>
            @Html.Partial("_BookList", Model)
        </div>
        <div class="col-md-3">
            @Html.Action("Categories", "Home")
            @Html.Action("DescendingList","Home")
        </div>
    </div>
</div>